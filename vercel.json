{
    "version": 2,
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
        {"src": "/(.*)", "dest": "server.js"},
        { "src": "/api/.*", "dest": "server.js" },
        { "src": "/api/docs/(.*)", "dest": "/api/docs" }
    ],
    "static": {"exclude": ["**/swagger-initializer.js"]}
}
